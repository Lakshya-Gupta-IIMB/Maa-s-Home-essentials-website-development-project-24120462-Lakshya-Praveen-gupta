        }
    });

    

    /****** 12th pass Exam details ******/  



    $('body').on('change','#candidatequalificationdetailstbl-twelthpassigstatus', function() {
       
        var pass = $('#candidatequalificationdetailstbl-twelthpassigstatus :selected').val();
        var tenPassYear = $.trim($('#candidatequalificationdetailstbl-tenthyearofpassing :selected').val());   
