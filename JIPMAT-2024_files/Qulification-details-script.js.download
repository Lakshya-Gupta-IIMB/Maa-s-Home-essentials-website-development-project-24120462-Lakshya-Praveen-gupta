$(document).ready(function () {   
    
    $('body').on('change','#candidatequalificationdetailstbl-tenthpassstatus', function() {
       
        var pass = $('#candidatequalificationdetailstbl-tenthpassstatus :selected').val();   
        if(pass==1)
        {
        $('#hideMode').show();
        $('#candidatequalificationdetailstbl-tenthyearofpassing').html('');
         
        } else if(!pass) {
            $('#candidatequalificationdetailstbl-tenthyearofpassing').html('');            
            $('#candidatequalificationdetailstbl-tenthyearofpassing').html('<option value="">--Select--</option>');
            $("#candidatequalificationdetailstbl-twelthpassigstatus").val("");
            $('#candidatequalificationdetailstbl-twelthyearofpassing').html('<option value="">--Select--</option>');
            $("#candidatequalificationdetailstbl-graduationpassigstatus").val("");
            $('#candidatequalificationdetailstbl-graduationyearofpassing').html('<option value="">--Select--</option>');
           
           }
           else {
           $('#hideMode').hide();
           $('#candidatequalificationdetailstbl-tenthyearofpassing').html('');           
           }
      
        $.ajax({
            url: "application/passyeartenth",
            type: 'post',
            data: {pass:pass},
            success: function (data) {
              if(!$.trim(data).length) { // zero-length string AFTER a trim
                 return false;
              }
               var totaldata=JSON.parse(data);
               if(pass==1){

                var i=0;
              
                $('#candidatequalificationdetailstbl-tenthyearofpassing').append('<option value="">--Select--</option>');
                $.each(totaldata, function(key, value){
                    
                    $('#candidatequalificationdetailstbl-tenthyearofpassing').append('<option value=' + totaldata[i]+ '>' + totaldata[i] + '</option>');
                    i++;
                });
                
               
             }
            else{
                var i=0;
               
                $('#candidatequalificationdetailstbl-tenthyearofpassing').append('<option value="">--Select--</option>');
                $.each(totaldata, function(key, value){
                    
                    $('#candidatequalificationdetailstbl-tenthyearofpassing').append('<option value=' + totaldata[i]+ '>' + totaldata[i] + '</option>');
                   
                    i++;
                });
             }

            }
       });


    });

     
      /* Add Percent Calculation  and CGPA here */ 
      $('body').on('change','#TenthResultMode', function() {
        var Percent = $('#TenthResultMode :selected').val();
       
        if(Percent!='')
        {
            if(Percent==1)
            {
              $('#changeMark').text('Marks(%)');
            }
            else {
              $('#changeMark').text('Marks(CGPA)');
            }

          $('#thentpercentage').show();
        }else {
            $('#thentpercentage').hide();

        }
    });


    /* Add other University here */ 
    $('body').on('change','#candidatequalificationdetailstbl-tenthuniversityboardid', function() {
