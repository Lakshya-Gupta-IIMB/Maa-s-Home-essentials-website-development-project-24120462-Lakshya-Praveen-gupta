          $("#candidatequalificationdetailstbl-tenthyearofpassing").addClass("is-invalid").attr("aria-invalid", true);
          $("#candidatequalificationdetailstbl-tenthyearofpassing").next(".invalid-feedback").html('Please select the 10 passed year').show();
          $("#candidatequalificationdetailstbl-tenthyearofpassing").focus();
          $("#candidatequalificationdetailstbl-twelthpassigstatus").val("");
          return false;
        } 
        if(pass==1 && tenPassYear!='')
        {
        $('#hideMode').show();
        $('#candidatequalificationdetailstbl-twelthyearofpassing').html('');
         
        } else if(!pass) {
            $('#candidatequalificationdetailstbl-twelthyearofpassing').html('');            
            $('#candidatequalificationdetailstbl-twelthyearofpassing').html('<option value="">--Select--</option>');
            $("#candidatequalificationdetailstbl-graduationpassigstatus").val("");
            $('#candidatequalificationdetailstbl-graduationyearofpassing').html('<option value="">--Select--</option>');
            }
           else {
           $('#hideMode').hide();
           $('#candidatequalificationdetailstbl-twelthyearofpassing').html('');           
           }
      
        $.ajax({
            url: "application/passyeartwelth",
            type: 'post',
            data: {pass:pass,tenPassYear:tenPassYear},
            success: function (data) { 
              if(!$.trim(data).length) { // zero-length string AFTER a trim
                return false;
             }           
               var totaldata=JSON.parse(data);
               if(pass==1){
                var i=0;              
                $('#candidatequalificationdetailstbl-twelthyearofpassing').append('<option value="">--Select--</option>');
                $.each(totaldata, function(key, value){
                    
                    $('#candidatequalificationdetailstbl-twelthyearofpassing').append('<option value="' + totaldata[i]+ '">' + totaldata[i] + '</option>');
                    i++;
                });
                
               
             }
            else{
                var i=0;               
                $('#candidatequalificationdetailstbl-twelthyearofpassing').append('<option value="">--Select--</option>');
                $.each(totaldata, function(key, value){                    
                    $('#candidatequalificationdetailstbl-twelthyearofpassing').append('<option value="' + totaldata[i]+ '">' + totaldata[i] + '</option>');
                    i++;
                });
             }
            }
       });
    });


