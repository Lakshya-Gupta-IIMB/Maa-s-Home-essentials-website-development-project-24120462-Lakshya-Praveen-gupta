 /****** 10th pass Exam details ******/  
 function passingyeartenth(optionSelect)
 {
     var pass = $('#candidatecorrectionwindowtbl-tenthpassstatus :selected').val();   
     if(pass==1)
     {
     $('#hideMode').show();
     $('#candidatequalificationdetailstbl-tenthyearofpassing').html('');
      
     } else if(!pass) {
         $('#candidatecorrectionwindowtbl-tenthyearofpassing').html('');            
         $('#candidatecorrectionwindowtbl-tenthyearofpassing').html('<option value="">--Select--</option>');
        
        }
        else {
        $('#hideMode').hide();
        $('#candidatecorrectionwindowtbl-tenthyearofpassing').html('');           
        }
 
        $.ajax({
         url: "passyear",
         type: 'post',
         data: {pass:pass},
         success: function (data) {
         
            var totaldata=JSON.parse(data);
            if(pass==1){
 
             var i=0;
           
             $('#candidatecorrectionwindowtbl-tenthyearofpassing').append('<option value="">--Select--</option>');
             $.each(totaldata, function(key, value){
                 
                 $('#candidatecorrectionwindowtbl-tenthyearofpassing').append('<option value="' + totaldata[i]+ '">' + totaldata[i] + '</option>');
                 i++;
             });
             
            
          }
         else{
             var i=0;
            
             $('#candidatecorrectionwindowtbl-tenthyearofpassing').append('<option value="">--Select--</option>');
             $.each(totaldata, function(key, value){
                 
                 $('#candidatecorrectionwindowtbl-tenthyearofpassing').append('<option value="' + totaldata[i]+ '">' + totaldata[i] + '</option>');
                
                 i++;
             });
          }
 
         }
    });
     
 
 }

