function openSelect(optionSelect)
{
    if(optionSelect==0)
    {
        $('#adharID').show();  
        $('#adharIDO').hide()        
    }  
    else if(optionSelect==1) {
        $('#adharID').show();  
        $('#adharIDO').show()   
    }
    else if(optionSelect==2)
    {
        $('#adharID').hide();  
        $('#adharIDO').hide();
    }
} 

  

